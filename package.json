{
  "name": "100sen-viewer",
  "version": "0.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && npm run serve",
    "compile": "cross-env NODE_ENV=\"development\" webpack --progress",
    "build": "rm -rf dist/ && cross-env NODE_ENV=\"product\" webpack --progress && electron-builder -wm",
    "serve": "electron .",
    "watch": "cross-env NODE_ENV=\"development\" webpack --progress --watch"
  },
  "build": {
    "appId": "com.electron.100sen-viewer",
    "mac": {
      "category": "public.app-category.utilities",
      "target": "dmg",
      "icon": "image/icon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.png"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ]
  },
  "keywords": [],
  "author": "fanta",
  "license": "MIT",
  "devDependencies": {
    "@types/dom-to-image": "^2.6.4",
    "@types/electron-prompt": "^1.6.1",
    "@types/node": "^17.0.34",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/request": "^2.48.8",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^18.2.3",
    "electron-builder": "^23.0.3",
    "electron-reload": "^2.0.0-alpha.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "csv": "^6.0.5",
    "dom-to-image": "^2.6.0",
    "electron-prompt": "^1.7.0",
    "node-fetch": "^3.2.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  }
}
